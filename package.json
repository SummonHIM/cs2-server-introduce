{
  "name": "l4d2-server-introduce",
  "productName": "SummonL4D2-Server-Launcher",
  "description": "A simple page to quickly connect to SummonHIM's Left 4 Dead 2 server.",
  "version": "1.0.0",
  "repository": "https://github.com/SummonHIM/l4d2-server-introduce",
  "author": {
    "email": "summonhim@qq.com",
    "name": "SummonHIM",
    "url": "https://www.summonhim.top"
  },
  "license": "MPL-2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:electron": "cross-env NODE_ENV=development concurrently -k \"vite\" \"wait-on tcp:5173 && electron-forge start\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "run-p type-check \"build-only {@}\" -- && electron-forge start"
  },
  "main": "src-electron/main.ts",
  "dependencies": {
    "@primeuix/themes": "^2.0.3",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.5",
