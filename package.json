    "electron-store": "^11.0.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "tailwindcss": "^4.1.18",
    "update-electron-app": "^3.1.2",
    "viewerjs": "^1.11.7",
    "vue": "^3.5.27",
    "vue-router": "^5.0.1"
  },
